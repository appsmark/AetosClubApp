<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo" loaded="onPageLoaded" backgroundColor="#731816">
	<ActionBar flat="true" backgroundColor="#731816">
		<GridLayout columns="*, 8*, *">
			<Image col="0" src="~/images/back.png" height="40vw" ontap="tapBack" visibility="{{visibility_button_back}}" />
			<Label col="1" class="title" text="{{textTop}}"/>
			<Image col="2" src="~/images/settings.png" height="40vw" ontap="tapSettings" visibility="{{visibility_button_settings}}" />
		</GridLayout>
	</ActionBar>
	<StackLayout>
	</StackLayout>
</Page>
